local count = 0
repeat
local args = {
    [1] = {
        [1] = "A",
        [2] = "\6"
    }
}

game:GetService("ReplicatedStorage"):WaitForChild("dataRemoteEvent"):FireServer(unpack(args))
wait(0.1)
local args = {
    [1] = {
        [1] = "A",
        [2] = "\6"
    }
}

game:GetService("ReplicatedStorage"):WaitForChild("dataRemoteEvent"):FireServer(unpack(args))
wait(0.8)
until (count >= Value)
