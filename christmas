local count = 0
local pos = game:GetService("Workspace").Main.Live.Ape.HumanoidRootPart.Position

local tp = game:GetService("Workspace").Main.Live.Ape.HumanoidRootPart.CFrame
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = tp

while (count <= 5000) do
    pos = game:GetService("Workspace").Main.Live.Ape.HumanoidRootPart.Position
    count=count+1 
    local args = {
    [1] = {
        ["Victim"] = workspace:WaitForChild("Main"):WaitForChild("Live"):WaitForChild("Ape"),
        ["Type"] = "Heavy",
        ["CurrentHeavy"] = 2,
        ["LocalInfo"] = {
            ["Flying"] = false
        },
        ["CurrentLight"] = 1,
        ["CurrentLightCombo"] = 1,
        ["VictimPosition"] = pos,
        ["AnimSet"] = "Broly"
        }
    }

    game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("TryAttack"):FireServer(unpack(args))
end
